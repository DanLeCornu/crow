{
  "expo": {
    "name": "Crow",
    "slug": "crow",
    "primaryColor": "#BABABA",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.2",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFE853"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/images/ios_icon.png",
      "bundleIdentifier": "co.noQuarter.crow",
      "buildNumber": "1.0.2",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses location data to show user location on map, and to set the bearing of the compass."
      }
    },
    "android": {
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "icon": "./assets/images/android_icon.png",
      "package": "co.noQuarter.crow",
      "versionCode": 4
    },
    "description": "Simple \"As the crow flies\" navigation app"
  }
}