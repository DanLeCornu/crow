{
  "expo": {
    "name": "Crow",
    "slug": "crow",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses location data to show user location on map"
      }
    },
    "description": "Simple \"As the crow flies\" navigation app",
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "no-quarter-co",
            "project": "crow",
            "authToken": "ae2cebc5ea264196aa6089cd5501b771dc228c9913c54278a0c72925c31b0943"
          }
        }
      ]
    }
  }
}
